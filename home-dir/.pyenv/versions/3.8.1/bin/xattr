#!/Users/captam3rica/.pyenv/versions/3.8.1/bin/python3.8
# EASY-INSTALL-ENTRY-SCRIPT: 'xattr==0.9.7','console_scripts','xattr'
__requires__ = 'xattr==0.9.7'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('xattr==0.9.7', 'console_scripts', 'xattr')()
    )
